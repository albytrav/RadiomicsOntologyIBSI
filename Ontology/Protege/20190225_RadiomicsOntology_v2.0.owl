        <rdfs:label>OpenSource</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.radiomics.org/RO/I0022 -->

    <owl:NamedIndividual rdf:about="http://www.radiomics.org/RO/I0022">
